<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	/* Add CSS here. See the GWT docs on UI Binder for more details */
	.important {
		font-weight: bold;
	}
	.RetrieveFusemail {
		border: 1px solid black;
		
	}
	.fusemailLabel {
		font-weight: bold;
	}
	.textAreas {
	}
	.backgroundColor {
		background-color: lavender;
	}
	</ui:style>
	<g:HTMLPanel styleName="{style.backgroundColor}">
		<g:VerticalPanel width="750px" height="307px" styleName="{style.RetrieveFusemail}">
			<g:HorizontalPanel width="748px" height="31px">
				<g:Cell horizontalAlignment="ALIGN_CENTER">
					<g:Label text="Retrieve Fusemail Message" styleName="{style.fusemailLabel}"/>
				</g:Cell>
			</g:HorizontalPanel>
			<g:Cell horizontalAlignment="ALIGN_CENTER">
				<g:HorizontalPanel height="31px" width="409px">
				<g:Cell horizontalAlignment="ALIGN_CENTER">
					<g:TextBox ui:field="serviceKeyField" styleName="textBoxBorder"/>
				</g:Cell>
				<g:Cell horizontalAlignment="ALIGN_CENTER">
					<g:TextBox ui:field="didField" styleName="textBoxBorder"/>
				</g:Cell>
			</g:HorizontalPanel>
			</g:Cell>
			<g:Cell horizontalAlignment="ALIGN_CENTER">
				<g:HorizontalPanel height="31px" width="405px">
					<g:Cell horizontalAlignment="ALIGN_CENTER">
						<g:TextBox ui:field="faxIdField" styleName="textBoxBorder"/>
					</g:Cell>
					<g:Cell horizontalAlignment="ALIGN_CENTER">
						<g:RadioButton name="new name" text="Inbound" ui:field="inboundRadio"/>
					</g:Cell>
				<g:Cell horizontalAlignment="ALIGN_CENTER">
					<g:RadioButton name="new name" text="Outbound" ui:field="outboundRadio"/>
				</g:Cell>
			</g:HorizontalPanel>
			</g:Cell>
			<g:Cell horizontalAlignment="ALIGN_CENTER">
				<g:HorizontalPanel width="745px" height="104px">
				<g:Cell horizontalAlignment="ALIGN_CENTER">
					<g:TextArea width="673px" height="98px" ui:field="resultText" styleName="textBoxBorder"/>
				</g:Cell>
			</g:HorizontalPanel>
			</g:Cell>
			<g:HorizontalPanel height="31px" width="745px">
				<g:Cell horizontalAlignment="ALIGN_CENTER">
					<g:Button ui:field="retrieveButton">Retrieve</g:Button>
				</g:Cell>
			</g:HorizontalPanel>
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 